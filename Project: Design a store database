SELECT * FROM floating_lockets_charms WHERE colour == "red" AND quantity >= 10; 
SELECT SUM(quantity) FROM floating_lockets_charms WHERE colour == "white";
SELECT supplier FROM floating_lockets_charms GROUP BY supplier; 
